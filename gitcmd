
yum install git -y

git --version

git config --global user.name "User_Name"
git config --global user.email "User_email ID"

git config list

mkdir gitrepo

git init

touch file{1,2,3,4}

ls 

file1 file2 file3 file4 

git status

git add .

git commit -m "commit msg"

git status

git remote -v

git remote add origin <URL of remote main repository> OR git clone <URL of remore reository>

git remote -v

git branch -M main

git branch -m <Old Branch Name> <New branch name>

git push -u origin main

git branch

git branch <branch name>

git push origin <Branch name>

git push origin --delete <Branch Name>

git push origin <New Branch Name>:main

git remote -v

git remote -a

git checkout <Branch Name>

git log

git log --oneline

git log <commit ID>

git show HEAD

git reset

git reset --hard

git revert

git revert HEAD

git pull OR git fetch

git checkout -b <Branch Name>

git checkout main

git merge <Branch name>




----------git stashing---------------

git stash 

git stash list

git stash apply stash@{No}

git stash clear



------------Git Reset-----------------

git reset

git reset <File name>

git reset --hard



Git reset  => Before commit
Git revert => After commit




-------------Git revert----------------

git revert

git revert HEAD

git revert <commit ID>
